<!doctype html>
<html lang="pt-br">
  <head>
    <!-- ======= Required Meta Tags ======= -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- ======= Bootstrap CSS ======= -->
    <link rel="stylesheet" href="/css/bootstrap.min.css">

    <!-- ======= Font Awesome ======= -->
    <link rel='stylesheet' href="/fontawesome/css/all.css">
